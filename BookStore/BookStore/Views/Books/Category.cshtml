@model IEnumerable<BookStore.Models.Book>

@{
    ViewBag.Title = "Category";
}

<table class="table">
 
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.Category)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                
                <td>
                    @Html.DisplayFor(modelItem => item.Category)
                </td>
                
            </tr>
        }

    </table>



